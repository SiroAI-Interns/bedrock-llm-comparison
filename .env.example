# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_DEFAULT_REGION=us-east-1

# Bedrock Model Regions
BEDROCK_CLAUDE_REGION=us-east-1
BEDROCK_TITAN_REGION=us-east-1
BEDROCK_LLAMA_REGION=us-east-1
BEDROCK_DEEPSEEK_REGION=us-west-2
BEDROCK_MISTRAL_REGION=us-east-1

# API Keys
OPENAI_API_KEY=your_openai_key
GEMINI_API_KEY=your_gemini_key

# Application Settings
LOG_LEVEL=INFO
MAX_WORKERS=5
CACHE_ENABLED=true

# Model Defaults
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=512

# Mistral-Specific Settings
MISTRAL_TOP_K=50
MISTRAL_TOP_P=0.9

# Pinecone Vector Database (optional, for cloud storage)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=us-east-1
